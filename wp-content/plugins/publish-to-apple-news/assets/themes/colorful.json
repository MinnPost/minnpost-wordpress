{
    "ad_frequency": 5,
    "ad_margin": 15,
    "blockquote_background_color": "#ffff00",
    "blockquote_border_color": "#3045ca",
    "blockquote_border_style": "none",
    "blockquote_border_width": 3,
    "blockquote_color": "#3045ca",
    "blockquote_font": "Georgia",
    "blockquote_line_height": 22,
    "blockquote_size": 17,
    "blockquote_tracking": 0,
    "body_background_color": "#e5feff",
    "body_color": "#000000",
    "body_font": "Optima-Regular",
    "body_line_height": 22,
    "body_link_color": "#3045ca",
    "body_orientation": "right",
    "body_size": 17,
    "body_tracking": 0,
    "author_color": "#000000",
    "author_font": "Optima-Regular",
    "author_format": "by #author#",
    "author_line_height": 22,
    "author_size": 13,
    "author_tracking": 21,
    "author_links": "no",
    "author_link_color": "#000000",
    "byline_color": "#000000",
    "byline_font": "Optima-Regular",
    "byline_format": "#F j, Y# | by #author#",
    "byline_line_height": 22,
    "byline_size": 13,
    "byline_tracking": 21,
    "caption_color": "#3045ca",
    "caption_font": "Georgia",
    "caption_line_height": 22,
    "caption_size": 14,
    "caption_tracking": -3,
    "date_color": "#000000",
    "date_font": "Optima-Regular",
    "date_format": "#F j, Y#",
    "date_line_height": 22,
    "date_size": 13,
    "date_tracking": 21,
    "dropcap_background_color": "#ffff00",
    "dropcap_color": "#000000",
    "dropcap_font": "HelveticaNeue-CondensedBlack",
    "dropcap_number_of_characters": 1,
    "dropcap_number_of_lines": 5,
    "dropcap_number_of_raised_lines": 0,
    "dropcap_padding": 5,
    "enable_advertisement": "yes",
    "gallery_type": "gallery",
    "header1_color": "#000000",
    "header1_font": "HelveticaNeue-CondensedBlack",
    "header1_line_height": 68,
    "header1_size": 72,
    "header1_tracking": -3,
    "header2_color": "#3045ca",
    "header2_font": "Georgia",
    "header2_line_height": 36,
    "header2_size": 36,
    "header2_tracking": -3,
    "header3_color": "#3045ca",
    "header3_font": "Georgia",
    "header3_line_height": 28,
    "header3_size": 28,
    "header3_tracking": -3,
    "header4_color": "#3045ca",
    "header4_font": "Georgia",
    "header4_line_height": 26,
    "header4_size": 26,
    "header4_tracking": -3,
    "header5_color": "#3045ca",
    "header5_font": "Georgia",
    "header5_line_height": 24,
    "header5_size": 24,
    "header5_tracking": -3,
    "header6_color": "#3045ca",
    "header6_font": "Georgia",
    "header6_line_height": 22,
    "header6_size": 22,
    "header6_tracking": -3,
    "initial_dropcap": "yes",
    "json_templates": {
        "title": {
            "default-title": {
                "backgroundColor": "#blockquote_background_color#",
                "fontName": "#header1_font#",
                "fontSize": "#header1_size#",
                "lineHeight": "#header1_line_height#",
                "tracking": "#header1_tracking#",
                "textColor": "#header1_color#",
                "textAlignment": "#text_alignment#",
                "textTransform": "uppercase"
            },
            "title-layout": {
                "columnSpan": 6,
                "columnStart": 1,
                "margin": {
                    "top": 30,
                    "bottom": 60
                }
            }
        },
        "author": {
            "default-author": {
                "backgroundColor": "#blockquote_background_color#",
                "textAlignment": "#text_alignment#",
                "fontName": "#author_font#",
                "fontSize": "#author_size#",
                "lineHeight": "#author_line_height#",
                "tracking": "#author_tracking#",
                "textColor": "#author_color#",
                "textTransform": "uppercase"
            },
            "author-layout": {
                "columnSpan": 6,
                "columnStart": 1,
                "margin": {
                    "top": 10,
                    "bottom": 10
                }
            }
        },
        "cover": {
            "headerBelowTextPhotoLayout": {
                "ignoreDocumentMargin": true,
                "columnStart": 0,
                "columnSpan": "#layout_columns#",
                "margin": {
                    "top": 30,
                    "bottom": 60
                }
            }
        },
        "image": {
            "json-with-caption": {
                "role": "container",
                "components": [
                    {
                        "role": "photo",
                        "URL": "#url#",
                        "layout": "#layout#",
                        "caption": "#caption#"
                    },
                    {
                        "role": "caption",
                        "text": "#caption#",
                        "textStyle": {
                            "textAlignment": "left",
                            "fontName": "#caption_font#",
                            "fontSize": "#caption_size#",
                            "tracking": "#caption_tracking#",
                            "lineHeight": "#caption_line_height#",
                            "textColor": "#caption_color#"
                        },
                        "layout": {
                            "columnSpan": 6,
                            "columnStart": 1,
                            "margin": {
                                "top": 20
                            },
                            "ignoreDocumentMargin": "#full_bleed_images#"
                        }
                    }
                ],
                "layout": {
                    "ignoreDocumentMargin": "#full_bleed_images#"
                }
            }
        },
        "table": {
            "default-table": {
                "border": {
                    "all": {
                        "color": "#table_border_color#",
                        "style": "#table_border_style#",
                        "width": "#table_border_width#"
                    }
                },
                "tableStyle": {
                    "cells": {
                        "backgroundColor": "#table_body_background_color#",
                        "horizontalAlignment": "#table_body_horizontal_alignment#",
                        "padding": "#table_body_padding#",
                        "textStyle": {
                            "fontName": "#table_body_font#",
                            "fontSize": "#table_body_size#",
                            "lineHeight": "#table_body_line_height#",
                            "textColor": "#table_body_color#",
                            "tracking": "#table_body_tracking#"
                        },
                        "verticalAlignment": "#table_body_vertical_alignment#"
                    },
                    "columns": {
                        "divider": {
                            "color": "#table_border_color#",
                            "style": "#table_border_style#",
                            "width": "#table_border_width#"
                        }
                    },
                    "headerCells": {
                        "backgroundColor": "#table_header_background_color#",
                        "horizontalAlignment": "#table_header_horizontal_alignment#",
                        "padding": {
                            "top": "#table_header_padding#",
                            "right": 5,
                            "bottom": "#table_header_padding#",
                            "left": 5
                        },
                        "textStyle": {
                            "fontName": "#table_header_font#",
                            "fontSize": "#table_header_size#",
                            "lineHeight": "#table_header_line_height#",
                            "textColor": "#table_header_color#",
                            "tracking": "#table_header_tracking#"
                        },
                        "verticalAlignment": "#table_header_vertical_alignment#"
                    },
                    "headerRows": {
                        "divider": {
                            "color": "#table_border_color#",
                            "style": "#table_border_style#",
                            "width": "#table_border_width#"
                        }
                    },
                    "rows": {
                        "divider": {
                            "color": "#table_border_color#",
                            "style": "#table_border_style#",
                            "width": "#table_border_width#"
                        }
                    }
                }
            }
        }
    },
    "layout_gutter": 20,
    "layout_margin": 100,
    "layout_width": 1024,
    "meta_component_order": [
        "title",
        "slug",
        "author",
        "date",
        "cover"
    ],
    "monospaced_color": "#000000",
    "monospaced_font": "Menlo-Regular",
    "monospaced_line_height": 20,
    "monospaced_size": 16,
    "monospaced_tracking": 0,
    "pullquote_border_color": "#3045ca",
    "pullquote_border_style": "none",
    "pullquote_border_width": 3,
    "pullquote_color": "#3045ca",
    "pullquote_font": "Georgia",
    "pullquote_hanging_punctuation": "no",
    "pullquote_line_height": 41,
    "pullquote_size": 36,
    "pullquote_tracking": -5,
    "pullquote_transform": "none",
    "slug_color": "#000000",
    "slug_font": "Optima-Regular",
    "slug_line_height": 26,
    "slug_size": 20,
    "slug_tracking": 0,
    "table_border_color": "#000000",
    "table_border_style": "solid",
    "table_border_width": 1,
    "table_body_background_color": "#e5feff",
    "table_body_color": "#000000",
    "table_body_font": "Optima-Regular",
    "table_body_horizontal_alignment": "left",
    "table_body_line_height": 22,
    "table_body_padding": 5.0,
    "table_body_size": 16,
    "table_body_tracking": 0,
    "table_body_vertical_alignment": "center",
    "table_header_background_color": "#ffff00",
    "table_header_color": "#000000",
    "table_header_font": "Optima-Bold",
    "table_header_horizontal_alignment": "center",
    "table_header_line_height": 22,
    "table_header_padding": 10.0,
    "table_header_size": 16,
    "table_header_tracking": 0,
    "table_header_vertical_alignment": "center",
    "theme_name": "Colorful"
}
